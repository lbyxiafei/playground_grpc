  "ReverseProxy": {
    "Routes": {
      "blank": {
        "ClusterId": "bbc",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "grpc-web-service": {
        "ClusterId": "grpc-web-service",
        "Match": {
          "Path": "grpc/grpc-web-service/{**catch-all}"
        },
        "transforms": [
          {
            "pathPattern": "{**catch-all}"
          },
          { "RequestHeadersCopy": "true" },
          { "RequestHeaderOriginalHost": "true" }
        ]
      }
    },
    "Clusters": {
      "bbc": {
        "Destinations": {
          "msn": {
            "Address": "https://www.bbc.com"
          }
        }
      },
      "grpc-web-service": {
        "Destinations": {
          "grpcServer": {
            "address": "http://localhost:5019/"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
